{
	"name": "secondsql",
	"version": "1.0.0",
	"description": "SQL language resources and snippets all in one website",
	"main": "index.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"clean": "rm -rf dist",
		"eleventy:default": "npx eleventy",
		"eleventy:watch": "npx eleventy --watch",
		"styles:prod": "node-sass src/styles/ --output dist/styles/ && postcss ./dist/styles/*.css --replace",
		"styles:dev": "node-sass src/styles/ --output dist/styles/ --source-map true --source-map-contents true && postcss ./dist/styles/*.css --replace",
		"scripts:prod": "webpack --mode=production",
		"scripts:dev": "webpack --mode=development",
		"serve": "light-server -c .lightserverrc",
		"dev": "npm-run-all clean styles:dev scripts:dev eleventy:default --parallel eleventy:watch serve --print-label",
		"dev:debug": "DEBUG=* npm run dev",
		"build": "run-s clean styles:prod scripts:prod eleventy:default --print-label"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Archeelux/SecondSQL.git"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Archeelux/SecondSQL/issues"
	},
	"homepage": "https://github.com/Archeelux/SecondSQL#readme",
	"devDependencies": {
		"light-server": "2.6.4"
	},
	"dependencies": {
		"@11ty/eleventy": "^0.9.0",
		"@babel/core": "^7.7.4",
		"@babel/preset-env": "^7.7.4",
		"acorn": "^7.1.0",
		"autoprefixer": "^9.7.2",
		"babel-loader": "^8.0.6",
		"babel-plugin-prismjs": "^1.1.1",
		"bulma": "^0.8.0",
		"cssnano": "^4.1.10",
		"cssnano-preset-advanced": "^4.0.7",
		"highlight.js": "^9.16.2",
		"i": "^0.3.6",
		"node-sass": "^4.13.0",
		"npm": "^6.13.4",
		"npm-run-all": "^4.1.5",
		"postcss-cli": "^6.1.3",
		"webpack": "^4.41.2",
		"webpack-cli": "^3.3.10"
	}
}
